# Tempo2
tempo2=true
tempo2.sync=active,iterative
tempo2.resolver=momentum

rtp.disable
ntp=false

# TOOD: change this
#db.location=R\:\\RADIXDB

# Network specific properties - Betanet Universe - P:30000


# TODO: need to be the bootstrap nodes IP
#network.seeds=192.168.0.10,13.72.79.113
#network.whitelist=192.168.*

# TODO: default this
# Explorer uses this API path as well
api.url=/api

# TODO: make default
api.functions=accounts,atoms,communications,graph,messages,network,system,transactions,universe

#db.discovery.query=1
#ledger.sync=true
core.modules=assets,atoms,transactions

# Only used to performance testing
# Shard space
partition.high=9223372036854775807
partition.low=-9223372036854775808

# Need to change for performance tests
partition.fragments=1

# Explorer nodes, needs lots of resources
#universe.witness=192.168.0.10,13.72.79.113

# Explorer nodes need this
universe.witness=1

# Explorer Port
cp.port=8080

# Square root of the network size (nodes) Minimum: 3
# TCP: sync ledger and temporal proof
network.connections.in=4
network.connections.out=4

# TODO: make default
ntp.pool=pool.ntp.org
# Safety mechanism
ntp=true

#atoms.spam.min_delay=0

# Turn on or off SystemProfiler. Defaults to false.
debug.profiler=false

# RTP
rtp.interval=4000000
rtp.age_threshold=5
rtp.decay = 0.3f
rtp.min_group_size=3
rtp.max_group_size=10
rtp.max_correction=1000
rtp.alpha_trim_factor=0.2f
rtp.target_coupling=0.1f
rtp.min_offset=5
rtp.max_deviation=25
rtp.max_start_deviation=10000
rtp.test=true
rtp.naughty=false
rtp.skew=0
rtp.clock_offset=0
